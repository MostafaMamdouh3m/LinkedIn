@model List<ApplicationUser>

<div id="nav_search_result_underlay" class="hidden">
    <div id="nav_search_result_board">
        @if (Model != null && Model.Count > 0)
        {
            for (int i = 0; i < Model.Count; i++)
            {
                <div userId="@Model[i].Id">@Model[i].FirstName @Model[i].MiddleName @Model[i].LastName</div>
            }
        }
        else
        {
            <div>No Results</div>
        }

    </div>
</div>
