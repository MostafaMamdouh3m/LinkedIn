@model List<KeyValuePair<string, Message>>

@if (Model != null && Model.Count != 0)
{
    <div class="row" id="message_chatters_contacts">
        @if (Model.Count != 0)
        {
            for (int i = 0; i < Model.Count; i++)
            {
                @Html.Partial("_PartialMessagePane", Model[i].Value)
            }
        }
    </div>
}
else
{
    <div class="row" id="message_chatters_contacts">
        <div class="message_chatters_contacts_empty">No Messages Found</div>
    </div>
}

