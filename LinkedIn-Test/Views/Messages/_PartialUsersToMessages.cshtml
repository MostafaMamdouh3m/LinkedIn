@model List<ApplicationUser>

@if (Model != null && Model.Count > 0)
{
    for (int i = 0; i < Model.Count; i++)
    {
        <div onclick="NewMessage('@Model[i].Id')">@Model[i].FirstName @Model[i].MiddleName @Model[i].LastName</div>
    }
}
else
{
    <div id="messages_no_user_found">NO USER FOUND</div>
}
